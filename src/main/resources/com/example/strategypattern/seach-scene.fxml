<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="384.0" prefWidth="445.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.strategypattern.MovieSearchSceneController">
   <children>
      <HBox alignment="CENTER" layoutX="42.0" layoutY="179.0" spacing="10">
      
          <!-- Search Field Label -->
          <Label text="Pesquisa:">
              <font>
                  <Font name="Arial" size="14" />
              </font>
          </Label>
      
          <!-- Text Field -->
          <TextField fx:id="searchField" prefWidth="200" />
      
          <!-- Search Button -->
          <Button fx:id="enterButton" text="Pesquisar" />
      </HBox>
      <Button fx:id="backHomeButton" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" text="Voltar para o inicio" />
   </children>
</AnchorPane>
